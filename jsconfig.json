{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"$components": ["src/components"],
			"$components/*": ["src/components/*"],
			"$utils": ["src/utils"],
			"$utils/*": ["src/utils/*"],
			"$stores": ["src/stores"],
			"$stores/*": ["src/stores/*"],
			"$actions": ["src/actions"],
			"$actions/*": ["src/actions/*"]
		}
	},
	"include": ["src/**/*.d.ts", "src/**/*.js", "src/**/*.svelte"]
}
